{
  "name": "react-bs4-forms",
  "version": "1.0.0",
  "description": "A Bootstrap 4 forms library for React",
  "main": "index.js",
  "scripts": {
    "build:lib": "./node_modules/.bin/babel --ignore *.spec.js src -d lib",
    "test": "npm run test:jest",
    "test:watch": "npm run test:jest -- --watch",
    "test:jest": "jest --verbose=true",
    "dev": "webpack-dev-server --hot --inline"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/eezing/react-bs4-forms.git"
  },
  "keywords": [
    "bootstrap",
    "4",
    "react",
    "forms",
    "fields",
    "css",
    "style",
    "responsive",
    "framework"
  ],
  "author": "Eric Zingeler",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/eezing/react-bs4-forms/issues"
  },
  "homepage": "https://github.com/eezing/react-bs4-forms#readme",
  "devDependencies": {
    "babel-cli": "^6.5.1",
    "babel-jest": "^12.0.2",
    "babel-loader": "^6.2.0",
    "babel-plugin-transform-runtime": "^6.8.0",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "bootstrap": "^4.0.0-alpha.2",
    "css-loader": "^0.23.1",
    "jest-cli": "^12.0.2",
    "node-sass": "^3.4.2",
    "react": "^15.0.2",
    "react-addons-test-utils": "^15.0.2",
    "react-dom": "^15.0.2",
    "react-hot-loader": "^1.3.0",
    "sass-loader": "^3.1.2",
    "style-loader": "^0.13.0",
    "webpack": "^1.12.14",
    "webpack-dev-server": "^1.14.0"
  },
  "jest": {
    "unmockedModulePathPatterns": [
      "<rootDir>/node_modules/react",
      "<rootDir>/node_modules/react-dom",
      "<rootDir>/node_modules/react-addons-test-utils"
    ]
  },
  "dependencies": {
    "babel-runtime": "^6.6.1"
  }
}
